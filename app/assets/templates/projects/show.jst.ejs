<h1>Project Detail</h1>
<div class="project-info"><div class="title">Title: <input class="title" type="text" value="<%= project.get('title') %>"></input></div>
<br>
<div class="description">Description: <input class="description" type="text" value="<%= project.get('description') %>"></input><div>
</div>
<br><br>
<div class="tasks">
Tasks: <br>
<% _.each(tasks, function(task, index) { %>
  <!-- <div class="task-show-link" data-id="<%= task.id %>"><a href="/tasks/<%= task.id %>"><%= task.get('title') %></a></div> -->
  <div class='task-show'>
  <div class="task-index"><%=index + 1%>.</div><div class="task-show-link" data-id="<%= task.id %>"><input class="task-input" data-id="<%= task.id %>" type="text" value="<%= task.get('title') %>"></input></div>
</div>
  
  <% }); %>
<div class="add-new-task">Add New Task</div>
</div>



